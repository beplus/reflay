{
  "presets": [ "es2015", "react", "stage-0" ],
  "env": {
    "test": {
      "plugins": [ [ "__coverage__", { "ignore": "test/**/*.js" } ] ]
    },
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
    "es": {
      "plugins": [
        "./build/use-lodash-es"
      ]
    }
  }
}
